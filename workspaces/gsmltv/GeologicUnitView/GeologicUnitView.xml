<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess 
	xmlns:as="http://www.geotools.org/app-schema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.geotools.org/app-schema http://svn.osgeo.org/geotools/trunk/modules/unsupported/app-schema/app-schema/src/test/resources/test-data/AppSchemaDataAccess.xsd">
	<namespaces>
		<Namespace>
			<prefix>gml</prefix>
			<uri>${gml.ns}</uri>
		</Namespace>
		<Namespace>
			<prefix>gsmltv</prefix>
			<uri>${gsmltv.ns}</uri>
		</Namespace>
		<Namespace>
			<prefix>xlink</prefix>
			<uri>${xlink.ns}</uri>
		</Namespace>
	</namespaces>
	<sourceDataStores>
		<DataStore>
			<id>gsmltv</id>
			<parameters>
				<Parameter>
					<name>dbtype</name>
					<value>postgis</value>
				</Parameter>
				<Parameter>
					<name>host</name>
					<value>${db.host}</value>
				</Parameter>
				<Parameter>
					<name>port</name>
					<value>${db.port}</value>
				</Parameter>
				<Parameter>
					<name>database</name>
					<value>${db.dbname}</value>
				</Parameter>
				<Parameter>
					<name>schema</name>
					<value>${db.schema}</value>
				</Parameter>
				<Parameter>
					<name>user</name>
					<value>${db.user}</value>
				</Parameter>
				<Parameter>
					<name>passwd</name>
					<value>${db.pass}</value>
				</Parameter>
			</parameters>
		</DataStore>
	</sourceDataStores>
	<catalog>../../../schemas/catalog.xml</catalog>
	<targetTypes>
		<FeatureType>
			<schemaUri>${gsmltv.xsd}</schemaUri>
		</FeatureType>
	</targetTypes>
	<typeMappings>
		<FeatureTypeMapping>
			<sourceDataStore>gsmltv</sourceDataStore>
			<sourceType>geologicunitview</sourceType>
			<targetElement>gsmltv:GeologicUnitView</targetElement>
			<attributeMappings>
				<!--IDENTIFICATION-->
				<AttributeMapping>
					<targetAttribute>gsmltv:GeologicUnitView</targetAttribute>
					<idExpression>
						<OCQL>getId()</OCQL>
					</idExpression>
				</AttributeMapping>
				<!--FEATURE URI-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:identifier</targetAttribute>
					<sourceExpression>
						<OCQL>featureuri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--FULL NAME-->	
				<AttributeMapping>
					<targetAttribute>gml:name[1]</targetAttribute>
					<sourceExpression>
						<OCQL>fullname</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>codeSpace</name>
						<value>'ncgmp:mapunitpolys:fullname'</value>
					</ClientProperty>
				</AttributeMapping>
				<!--LABEL-->	
				<AttributeMapping>
					<targetAttribute>gml:name[2]</targetAttribute>
					<sourceExpression>
						<OCQL>label</OCQL>
					</sourceExpression>
					<ClientProperty>
						<name>codeSpace</name>
						<value>'ncgmp:mapunitpolys:label'</value>
					</ClientProperty>
				</AttributeMapping>
				<!--NAME-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:name</targetAttribute>
					<sourceExpression>
						<OCQL>name</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--DESCRIPTION-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:description</targetAttribute>
					<sourceExpression>
						<OCQL>description</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--SYMBOL-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:genericSymbolizer</targetAttribute>
					<sourceExpression>
						<OCQL>symbol</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--SPECIFICATION URI-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:specification_uri</targetAttribute>
					<sourceExpression>
						<OCQL>specificationuri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--GEOLOGIC UNIT TYPE-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:geologicUnitType</targetAttribute>
					<sourceExpression>
						<OCQL>geologicunittype</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--GEOLOGIC HISTORY-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:geologicHistory</targetAttribute>
					<sourceExpression>
						<OCQL>geologichistory</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--REPRESENTATIVE AGE-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:representativeAge_uri</targetAttribute>
					<sourceExpression>
						<OCQL>representativeageuri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--YOUNGER AGE URI-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:representativeLowerAge_uri</targetAttribute>
					<sourceExpression>
						<OCQL>youngerageuri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--OLDER AGE URI-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:representativeUpperAge_uri</targetAttribute>
					<sourceExpression>
						<OCQL>olderageuri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--RANK-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:rank</targetAttribute>
					<sourceExpression>
						<OCQL>rank</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--LITHOLOGY-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:lithology</targetAttribute>
					<sourceExpression>
						<OCQL>lithology</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--REPRESENTATIVE LITHOLOGY-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:representativeLithology_uri</targetAttribute>
					<sourceExpression>
						<OCQL>representativelithologyuri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--OBSERVATION METHOD-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:observationMethod</targetAttribute>
					<sourceExpression>
						<OCQL>observationmethod</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--SOURCE-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:source_uri</targetAttribute>
					<sourceExpression>
						<OCQL>sourceuri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--METADATA-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:metadata_uri</targetAttribute>
					<sourceExpression>
						<OCQL>metadatauri</OCQL>
					</sourceExpression>
				</AttributeMapping>
				<!--SHAPE-->	
				<AttributeMapping>
					<targetAttribute>gsmltv:shape</targetAttribute>
					<sourceExpression>
						<OCQL>shape</OCQL>
					</sourceExpression>
				</AttributeMapping>
			</attributeMappings>
		</FeatureTypeMapping>
	</typeMappings>
</as:AppSchemaDataAccess>